							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Related_Examiners" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Related_Examiner" type="dfax:RelatedExaminerType" maxOccurs="unbounded"/>
