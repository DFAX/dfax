						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!---->
	<xs:complexType name="AttorneyType">
		<xs:complexContent>
			<xs:extension base="ucoCommon:IdentityConstructType">
