							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Authorizations" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Authorization" type="dfax:AuthorizationType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Investigators" minOccurs="0">
