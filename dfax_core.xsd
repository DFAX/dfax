					</xs:element>
					<xs:element name="Forensic_Actions" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Forensic_Action" type="dfax:ForensicActionType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Evidence_Records" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Evidence_Record" type="dfax:EvidenceRecordType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Related_Cases" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Related_Case" type="dfax:RelatedCaseType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!---->
	<xs:complexType name="SubjectType">
		<xs:complexContent>
			<xs:extension base="ucoCommon:IdentityConstructType">
